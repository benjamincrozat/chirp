{
    "name": "benjamincrozat/chirp",
    "description": "A free Twitter activity tracker based on Laravel and running on AWS Lambda.",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Crozat",
            "email": "benjamincrozat@me.com"
        }
    ],
    "keywords": [
        "aws",
        "dynamodb",
        "lambda",
        "laravel",
        "sqs"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "packages/twitteroauth"
        }
    ],
    "require": {
        "php": "^7.4",
        "abraham/twitteroauth": "dev-master",
        "blade-ui-kit/blade-zondicons": "^0.1.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/dusk": "^6.2",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "sentry/sentry-laravel": "^1.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.14.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/laravel-mojito": "^0.2.4",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan dusk",
            "@php vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
