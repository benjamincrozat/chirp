{
    "name": "benjamincrozat/chirp",
    "description": "A free Twitter activity tracker based on Laravel and running on AWS Lambda.",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Crozat",
            "email": "benjamincrozat@me.com"
        }
    ],
    "keywords": [
        "aws",
        "dynamodb",
        "lambda",
        "laravel",
        "sqs"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "packages/twitteroauth"
        }
    ],
    "require": {
        "php": "^8.0",
        "abraham/twitteroauth": "dev-master",
        "blade-ui-kit/blade-zondicons": "^1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/dusk": "^6.15",
        "laravel/framework": "^8.38",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "sentry/sentry-laravel": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.14.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/laravel-mojito": "^0.2.4",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan dusk",
            "@php vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
